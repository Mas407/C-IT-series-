 int fibbonic(int n){
     if(n==0)return 0;
     if(n==1)return 1;
     return fibbonic(n-1)+fibbonic(n-2);
 }
 int main(){
     int n;
     cout<<"Enter the number ";
     cin>>n;
     cout<<"Number is fibbonic "<<fibbonic(n);



 bool prime(int n){
     if(n<2)return false;
     for(int i=2; i<=sqrt(n); i++){
     if(n%i==0)return false;}
     return true;
 }
 int main(){
     int n;
     cout<<"Enter the number ";
     cin>>n;
     if(prime(n))
     cout<<"Number is prime ";
     else 
     cout<<"Not a prime number ";



