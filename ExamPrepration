 //Binary search 
 int arr[5]={10,20,30,40,50};
 int start=0,end=4,mid,loc=-1,n;
 cout<<"Enter the number ";
 cin>>n;
 while(start<=end){
     mid=(start+end)/2;
     if(arr[mid]==n){
         loc=mid;
         break;
     }
     if(n<arr[mid])
     end=mid-1;
     else 
     start=mid+1;
 }
 if(loc!=-1)
 cout<<"The value is at index "<<loc;
 else 
 cout<<"invalid";

