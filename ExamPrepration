 int call,bill,n=200;
 cout<<"Enter the numbers of calls ";
 cin>>call;
 if(call<=100)
 bill=n;
 else if(call<=150)
 bill=n+(call-100)*0.60;
 else if(call<=200)
 bill=n+(50*0.60)+(call-150)*0.50;
 else if(call<=250)
 bill=n+(50*0.60)+(50*0.50)+(call-200)*0.40;
 cout<<"Total bill is "<<bill;

