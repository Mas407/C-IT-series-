 switch(n){
     case 'a':
     case 'A':
     cout<<"Vowels";
     break;
     case 'e':
     case 'E':
     cout<<"Vowels";
     break;
     case 'i':
     case 'I':
     cout<<"Vowels";
     break;
     case 'o':
     case 'O':
     cout<<"Vowels";
     break;
     case 'u':
     case 'U':
     cout<<"Vowels";
     break;
 default:
 cout<<"consunats ";



