 }


 int n,fac=1;
 cout<<"Enter the number ";
 cin>>n;
 for(int i=1; i<=n; i++){
     fac*=i;
 }
 cout<<fac;

int arr[3][3];
int i,j;
cout<<"Enter the matrix :";
for( i=0; i<3; i++)
for( j=0; j<3; j++)
cin>>arr[i][j];
cout<<"the value in i and j is "<<arr[i][j];
 return 0;
 }
